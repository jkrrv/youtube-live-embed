{
    "name": "jkrrv/youtube-live-embed",
    "description": "Find and embed Live YouTube videos for a given channel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Kurtz",
            "email": "jkrrv@jkrrv.org"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jkrrv/youtube-live-embed"
        }
    ],
    "autoload": {
        "psr-4": {
            "jkrrv\\": "src/",
            "jkrrv\\YouTubeLiveEmbed\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            ".idea/",
            "tests/TestCredentials.php"
        ]
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*"
    }
}
